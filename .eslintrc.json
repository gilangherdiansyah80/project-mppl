{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-alert": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "comma-dangle": 0,

        "no-unused-vars": 0,
        "quotes": 0,
        "no-multiple-empty-lines": 0,
        "prefer-const": 0,
        "indent": 0,
        "no-plusplus": 0,
        "eqeqeq": 0,
        " prefer-arrow-callback": 0,
        "func-names": 0,
        "arrow-parens": 0,
        "implicit-arrow-linebreak": 0,
        "function-paren-newline": 0,
        "no-use-before-define": 0,
        "semi": 0,
        "prefer-arrow-callback": 0,
        "max-len": 0,
        "padded-blocks": 0,
        "eol-last": 0,

        "no-undef": 0,
        "space-before-function-paren": 0,
        "no-trailing-spaces": 0
    }
}
